{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Enable QuizletHacker!"
   },
   "description": "Tool for Quizlet to help you get the highest scores!",
   "icons": {
      "128": "icon_l.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "QuizletHacker",
   "permissions": [ "activeTab", "tabs" ],
   "short_name": "QuizletHacker",
   "version": "1.4.3",
   "web_accessible_resources": ["ScatterHacker.js", "GravityHacker.js", "LearnHacker.js"]
}
